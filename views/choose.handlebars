<h1>Choose articles from fark.com!</h1>


<ul>
  {{#each results}}
    <div class = "article" id = "{{this.number}}">
        <p id="title{{this.number}}">{{this.title}}</p>
        <p id="link{{this.number}}">{{this.link}}</p>
        <button  id="{{this.number}}" class="select_article">Save</button>
    </div>
  {{/each}}
</ul>
